{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // disable non underscored dangling underscore
        "no-underscore-dangle": "off",
        "no-console": "off"
    },
    "ignorePatterns":[
        "node_modules/*",
        "dist/*",
        "karma.conf.js",
        "webpack.*.js",
        "specs/*"
    ]
}
